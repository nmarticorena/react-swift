    "@rollup/plugin-url": "^6.0.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.12.38",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "@types/sass": "^1.16.0",
    "@types/three": "^0.127.1",
