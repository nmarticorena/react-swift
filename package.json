  "repository": "jhavl/react-swift",
  "type": "module",
  "main": "dist/index",
  "module": "dist/index",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "run-s test:unit test:lint test:build",
